    <body>
        <!-- include header -->
        <nav class="navbar sticky-top navbar-expand-lg white">
            <!-- Navbar brand -->
            <ul class="navbar-nav mr-auto">
                <li class="nav-item">
                    <a class="nav-link p-0 brand" href="/home">
                        <img src="images/one1000-primary-logo.png" class="z-depth-0" alt="avatar image" height="90">
                    </a>
                </li>
            </ul>
        
            <!-- Collapse button -->
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#basicExampleNav"
                aria-controls="basicExampleNav" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
                <i class="fas fa-bars"></i>
            </button>
        
            <!-- Collapsible content -->
            <div class="collapse navbar-collapse" id="basicExampleNav">
                <!-- Links -->
                <ul class="navbar-nav mr-auto ml-lg-5 pl-lg-5">
                    <% if (sess_email){ %>
                        <li class="nav-item">
                            <a class="nav-link text-dark" href="/userDashboard">Dashboard</a>
                        </li>
                    <% } %>
                    <li class="nav-item">
                        <a class="nav-link active text-dark" href="/home">Home
                            <span class="sr-only">(current)</span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-dark" href="/webinar">Webinar</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-dark" href="/onlinetraining">Online Training</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-dark" href="/about">About us</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-dark" href="/contact">Contact us</a>
                    </li>
                </ul>
                <!-- Links -->
        
                <!-- Collapsible content -->
                <% if (!sess_email){ %>
                <a href="/login" style="border-radius: 15px; background: #017EBE;" class="btn text-white btn-md ml-3">Login</a>
                <% }  else { %>
                    <a href="/logout" style="border-radius: 15px; background: #017EBE;" class="btn text-white btn-md ml-3">Logout</a>
                <% } %>
            </div>
        </nav>
        <!-- /include header -->

        <div class="container container-bg">
            <div class="row">
                <div class="col-lg-5 col-md-5 col-sm-5 custom-col">
                    <div>
                        <h2 style="font-weight: bold; margin: 0;">
                            <span class="span-bg ml-n3">We can help</span> 
                            <span>you acheive</span>
                            <span class="d-flex">excellence
                                <img src="images/writing-0.5.png" class="z-depth-0 ml-n2 mt-2 img-fluid" alt="avatar image">
                            </span>
                        </h2>
                    </div>
                    
                    <div class="row">
                        <h6 class="h6-header mt-5">WES is a worldwide provider of online training courses. We provide a wide range of Social Services, Business, Government and Life Skills courses.</h6>
                    </div>
                    <div class="row mt-5">
                        <div class="col-lg-6">
                            <div class="text-center mb-4">
                                <a href="/contact" class="btn btn-outline-primary btn-block text-center" style="color:#017EBE; border-radius: 15px;" name="contact">Contact Us</a>
                            </div>
                        </div>
                        <div class="col-lg-6">
                            <% if (!sess_email){ %>
                            <div class="text-center mb-4">
                                <a href="/signup" class="btn text-center btn-block" style="color: white; border-radius: 15px; background: #017EBE;" name="getstarted">Get Started</a>
                            </div>
                            <% } %>
                        </div>
                    </div>
                </div>
                <div class="col-lg-7 col-md-7 col-sm-7 col-header-img">
                    <div class=" text-right header-img-div">
                        <img src="images/Rectangle-16.png" class="z-depth-0 header-img img-fluid" alt="avatar image">
                    </div>
                </div>
            </div>
            
        </div>

        <!-- Logos -->
        <div class="container">
            <div class="row">
                <div class="col-lg-2 col-4">
                    <img src="images/004_Noth_York-1.png" class="z-depth-0 img-fluid" alt="avatar image">

                </div>
                <div class="col-lg-2 col-4">
                    <img src="images/004_Noth_York-2.png" class="z-depth-0 img-fluid" alt="avatar image">

